{
  "name": "Java Development",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
      "ghcr.io/devcontainers/features/git:1": {
          "version": "latest",
          "ppa": "false"
      },
      "ghcr.io/devcontainers/features/java:1": {
          "version": "21",
          "jdkDistro": "amzn",
          "installMaven": "true",
          "mavenVersion": "3.9.7",
          "installGradle": "false"
      }
  },
  "containerEnv": {},
  "customizations": {
      "vscode": {
          "settings": {},
          "extensions": [
              "vscjava.vscode-java-pack"
          ]
      }
  }
}